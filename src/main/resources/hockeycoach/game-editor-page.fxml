<?import javafx.scene.layout.Pane?>

<Pane maxHeight="820.0" maxWidth="1200.0" minHeight="820.0" minWidth="1200.0" prefHeight="820.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hockeycoach.controllers.GameEditorController">
    <children>
        <TabPane fx:id="lineupTabPane" layoutX="299.0" layoutY="67.0" prefHeight="748.0" prefWidth="590.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="lineupTab" text="Lineup">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="698.0" prefWidth="895.0">
                     <children>
                        <ImageView fx:id="boardImage" fitHeight="722.0" fitWidth="590.0" pickOnBounds="true" preserveRatio="true" />
                     </children></AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="powerplayTab" text="Powerplay">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab fx:id="BoxplayTab" text="Boxplay">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <Label layoutX="12.0" layoutY="67.0" prefHeight="25.0" prefWidth="64.0" text="Date" />
        <TextField fx:id="gameDate" layoutX="77.0" layoutY="64.0" prefHeight="30.0" prefWidth="213.0" />
        <TextField fx:id="gameTime" layoutX="77.0" layoutY="94.0" prefHeight="30.0" prefWidth="213.0" />
        <Label layoutX="12.0" layoutY="97.0" prefHeight="25.0" prefWidth="64.0" text="Time" />
        <TextField fx:id="gameStadium" layoutX="77.0" layoutY="124.0" prefHeight="30.0" prefWidth="213.0" />
        <Label layoutX="12.0" layoutY="127.0" prefHeight="25.0" prefWidth="64.0" text="Stadium" />
        <TextField fx:id="gameTeam" layoutX="77.0" layoutY="152.0" prefHeight="30.0" prefWidth="213.0" />
        <Label layoutX="12.0" layoutY="155.0" prefHeight="25.0" prefWidth="64.0" text="Team" />
        <TextField fx:id="gameOpponent" layoutX="77.0" layoutY="182.0" prefHeight="30.0" prefWidth="213.0" />
        <Label layoutX="12.0" layoutY="185.0" prefHeight="25.0" prefWidth="64.0" text="Opponent" />
        <HBox fx:id="controlBox" alignment="TOP_RIGHT" layoutX="5.0" layoutY="7.0" prefHeight="41.0" prefWidth="1190.0" spacing="5.0">
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" text="Save" />
                <Button fx:id="cancelButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" text="Cancel" />
            </children>
        </HBox>
      <TableView fx:id="teamPlayers" layoutX="14.0" layoutY="297.0" prefHeight="518.0" prefWidth="275.0">
         <columns>
            <TableColumn fx:id="playerLastName" minWidth="150.0" prefWidth="150.0" text="Last Name">
               <cellValueFactory>
                  <PropertyValueFactory property="lastName" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="playerFirstName" minWidth="125.0" prefWidth="125.0" text="First Name">
               <cellValueFactory>
                  <PropertyValueFactory property="firstName" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
      <TableView fx:id="availablePlayers" layoutX="895.0" layoutY="67.0" prefHeight="385.0" prefWidth="300.0">
         <columns>
            <TableColumn fx:id="avPlayerLastName" minWidth="130.0" prefWidth="130.0" text="Last Name">
               <cellValueFactory>
                  <PropertyValueFactory property="lastName" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="avPlayerFirstName" minWidth="100.0" prefWidth="100.0" text="First Name">
               <cellValueFactory>
                  <PropertyValueFactory property="firstName" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="avPlayerPosition" minWidth="70.0" prefWidth="70.0" text="Position">
               <cellValueFactory>
                  <PropertyValueFactory property="positions" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
    </children>
</Pane>
